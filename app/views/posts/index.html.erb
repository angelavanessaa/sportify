<div class= "centering">
	<% @posts.each do |x| %>
		<div class= "centering">
		    <figure class="snip1423">
		      <img src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/331810/sample57.jpg" alt="sample57" />
		      <figcaption>
		        <h3> 
		             <%=x.product.name%>
		        </h3>
		        <p><%=x.product.description%></p>
		        <div class="price">
		          <s><%=x.price%></s><%=x.price%>
		        </div>
		        <div>
		            <%=x.end_date%>
		        </div>
		      </figcaption><i class="ion-android-cart"></i>
		      <a href="#"></a>
		    </figure>
		</div>
	<% end %>
</div>




